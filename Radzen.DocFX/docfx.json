{
  "metadata": [
    {
      "src": [
        {
          "files": ["Radzen.Blazor.csproj"],
          "src": "../Radzen.Blazor/"
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": ["api/**.yml", "api/index.md"]
      },
      {
        "files": ["articles/**.md", "articles/**/toc.yml", "toc.yml", "*.md"]
      }
    ],
    "resource": [
      {
        "files": ["images/**"]
      }
    ],
    "overwrite": [
      {
        "files": ["apidoc/**.md"],
        "exclude": ["obj/**", "_site/**"]
      }
    ],
    "dest": "../RadzenBlazorDemos/wwwroot/docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": ["templates/radzen"],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
